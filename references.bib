@article{kunertova2017,
  title = {One Measure Cannot Trump It All: Lessons from {{NATO}}’s Early Burden-Sharing Debates},
  shorttitle = {One Measure Cannot Trump It All},
  author = {Kunertova, Dominika},
  date = {2017-10-02},
  journaltitle = {European Security},
